extends base

block content
    .logout-user
        p.text-main MY ACCOUNT
        .logout-user-wrapper
            ul
                li.login-selected
                    a(href='/account/details/') My Account Details
                li.account-navigation--wishlist
                    a.link-active(href='/account/wishlist/') My Wishlist
                li
                    a(href='/account/orders/') My Orders
                li
                    a(href='/account/address-book/')  Address Book
                li.logout
                    a#logout-clicked.logout-link.link-active(href='') logout
        //- p.text-main  MY WISHLIST
        //- p.fine-secondary There are currently no items in your wishlist
        .whishlist.whislist-hig
        .whishlist-container
            .whishlist-container-title
                .whishlist-container-title-content
                    p.text-main MY WISHLIST
                    p.text-error.errorMsgWhishlist
                        | Your wishlist will be lost at the
                        | end of your browsing session. Please sign in or register
                        | in order to save your wishlist.
                    p.text-error.errorMsgWhishlist There are currently no items in your wishlist
