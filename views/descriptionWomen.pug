                    //- +overviewBox()
                    each img, i in women.images
                        .showcase-loads-container-gallery-left-content
                            img.image-gallery-left(src=`/img/${img}` alt=`${i + 1}`)
                .showcase-loads-container-gallery-right
                    .showcase-loads-container-gallery-right-details
